{
  "name": "bs-grpc",
  "version": "0.1.0",
  "scripts": {
    "clean": "bsb -clean-world",
    "build": "npm run compile-static-protobuf-compiler-protobufs",
    "watch": "bsb -make-world -w",
    "compile-static-protobuf-compiler-protobufs": "./node_modules/.bin/pbjs -t static-module -p node_modules/grpc-tools/bin/google/protobuf node_modules/grpc-tools/bin/google/protobuf/compiler/plugin.proto > src/protobufs.js",
    "compile-static-reasonml-bindings": "PATH=\"./bin:$PATH\" ./node_modules/.bin/grpc_tools_node_protoc example/hello.proto --reasonml_out=src"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "^3.0.0",
    "grpc-tools": "^1.6.6",
    "grpc": "^1.11.3"
  },
  "peerDependencies": {
    "bs-platform": "^3.0.0",
    "grpc-tools": "^1.6.6"
  },
  "dependencies": {
    "@grpc/grpc-js": "^0.1.0",
    "protobufjs": "^6.8.6",
    "read-all-stream": "^3.1.0"
  }
}
